<!DOCTYPE html>

<html ng-app="shapeApp">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Shaping APIs</title>

    <link rel="stylesheet" href="~/lib/perfect-scrollbar/min/perfect-scrollbar.min.css" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/styles/site.css" rel="stylesheet" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/app/app.js"></script>

    <script src="~/lib/perfect-scrollbar/min/perfect-scrollbar.min.js"></script>
    <script src="~/lib/angular-perfect-scrollbar/src/angular-perfect-scrollbar.js"></script>
    <script src="~/lib/ng-json-prettifier/dist/ng-json-prettifier.js"></script>
</head>
<body>
    <nav id="myNavbar" class="navbar navbar-default navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="http://chsakell.com/" target="_blank">chsakell's blog</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a class="fa fa-info-circle" href="http://wp.me/p3mRWu-15c" target="_blank">About</a></li>
                    <li><a class="fa fa-music" href="@Url.Action("Index")">Tracks</a></li>
                    <li><a class="fa fa-folder-open " href="@Url.Action("Albums")">Albums</a></li>
                    <li><a class="fa fa-user" href="@Url.Action("Artists")">Artists</a></li>
                    <li><a class="fa fa-users" href="@Url.Action("Customers")">Customers</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <div class="container">
            <div class="row">
                <h3 class="text-center"><a href="#" target="_blank">Building multi-client APIs with ASP.NET</a></h3>
                <p class="text-center">
                    <a href="https://twitter.com/chsakellsblog" target="_blank">
                        <img src="~/images/twitter.png" class="blog-image" />
                    </a>
                    <a href="https://github.com/chsakell" target="_blank">
                        <img src="~/images/github.png" class="blog-image" />
                    </a>
                    <a href="https://www.facebook.com/chsakells.blog" target="_blank">
                        <img src="~/images/facebook.png" class="blog-image" />
                    </a>
                </p>
            </div>
        </div>
        @RenderBody()
    </div>
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        @RenderSection("customScript", required: false)
    </script>
</body>
</html>